---
net: <%= node[:ipaddress] %>
port: <%= node[:cloudfoundry_common][:nats_server][:port] %>

pid_file: <%= node[:nats_server][:pid_file] %>
log_file: <%= node[:nats_server][:log_file] %>

authorization:
  user: <%= node[:cloudfoundry_common][:nats_server][:user] %>
  password: <%= node[:cloudfoundry_common][:nats_server][:password] %>
  timeout: 20
